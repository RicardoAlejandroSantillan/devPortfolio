<h2 data-translate="projects.title">Experiencia</h2>

<div class="project-filter">
    <button class="project-filter-btn active" id="professional-work-btn" data-translate="projects.professional">Trabajos</button>
    <button class="project-filter-btn" id="personal-project-btn" data-translate="projects.personal">Proyectos Personales</button>
</div>

<section class="carouseles-container">

    <!-- Carrusel de Professional Work -->
    <div class="professional-work">
        <div class="carousel-professional-work-container">
            <!-- CHEVRONES DE NAVEGACIÓN -->
            <button class="carousel-professional-work-arrow carousel-arrow left" aria-label="Anterior">
                <svg viewBox="0 0 24 24">
                    <path d="M15.41 7.41L14 6l-6 6 6 6 1.41-1.41L10.83 12z" />
                </svg>
            </button>
            <button class="carousel-professional-work-arrow carousel-arrow right" aria-label="Siguiente">
                <svg viewBox="0 0 24 24">
                    <path d="M10 6L8.59 7.41 13.17 12l-4.58 4.59L10 18l6-6z" />
                </svg>
            </button>

            <!-- PISTA DE TARJETAS -->
            <div class="carousel-track carousel-professional-work-track">
                <!-- TARJETAS ORIGINALES -->
                @* Daikin *@
                <div class="carousel-professional-work-card" data-project="Daikin">
                    <img src="~/images/projects/Daikin/cover.webp"
                         alt="Daikin">
                    <div class="work-information-content">
                        <h3 data-translate="projects.daikin.name">Daikin</h3>
                        <p data-translate="projects.daikin.period">Septiembre 2024 - Diciembre 2025</p>
                        <p data-translate="projects.daikin.type">Periodo de Practicas</p>
                    </div>
                </div>
            </div>

            <!-- PAGINACIÓN -->
            <div class="carousel-pagination carousel-professional-work-pagination">
                <span class="current">1</span>/<span class="total">1</span>
            </div>
        </div>
    </div>



    <!-- Carrusel de Personal Projects -->
    <div class="personal-project">
        <div class="carousel-personal-project-container">
            <!-- CHEVRONES DE NAVEGACIÓN -->
            <button class="carousel-personal-project-arrow carousel-arrow left" aria-label="Anterior">
                <svg viewBox="0 0 24 24">
                    <path d="M15.41 7.41L14 6l-6 6 6 6 1.41-1.41L10.83 12z" />
                </svg>
            </button>
            <button class="carousel-personal-project-arrow carousel-arrow right" aria-label="Siguiente">
                <svg viewBox="0 0 24 24">
                    <path d="M10 6L8.59 7.41 13.17 12l-4.58 4.59L10 18l6-6z" />
                </svg>
            </button>

            <!-- PISTA DE TARJETAS -->
            <div class="carousel-track carousel-personal-project-track">
                <!-- TARJETAS ORIGINALES -->
                @* DevPortfolio *@
                <div class="carousel-personal-project-card carousel-card" data-project="devportfolio">
                    <img src="~/images/projects/DevPortfolio/portfolio-cover.webp"
                         alt="devportfolio">
                    <div class="personal-project-content">
                        <h3 data-translate="projects.devportfolio.name">DevPortfolio</h3>
                        <p data-translate="projects.devportfolio.shortDescription">Portafolio profesional de desarrollador con diseño espacial y glasmorfismo</p>
                    </div>
                </div>

                @* Datathon 2022 *@
                <div class="carousel-personal-project-card carousel-card" data-project="datathon2022">
                    <img src="~/images/projects/Datathons/WiDS.webp"
                         alt="WiDS For Woman Datathon2022">
                    <div class="personal-project-content">
                        <h3 data-translate="projects.datathon2022.name">WiDS Datathon 2022</h3>
                        <p data-translate="projects.datathon2022.shortDescription">Datathon enfocado en temas de salud</p>
                    </div>
                </div>

                @* Datathon 2025 *@
                <div class="carousel-personal-project-card carousel-card" data-project="datathon2025">
                    <img src="~/images/projects/Datathons/WiDS.webp"
                         alt="WiDS For Woman Datathon 2025">
                    <div class="personal-project-content">
                        <h3 data-translate="projects.datathon2025.name">WiDS Datathon 2025</h3>
                        <p data-translate="projects.datathon2025.shortDescription">Datathon enfocado en temas de salud</p>
                    </div>
                </div>

                @* BookShelf *@
                <div class="carousel-personal-project-card carousel-card" data-project="bookshelf">
                    <img src="~/images/projects/BookShelf/cover.webp"
                         alt="bookshelf">
                    <div class="personal-project-content">
                        <h3 data-translate="projects.bookshelf.name">BookShelf</h3>
                        <p data-translate="projects.bookshelf.shortDescription">Portafolio profesional de desarrollador con diseño espacial y glasmorfismo</p>
                    </div>
                </div>

                @* proSalud *@
                <div class="carousel-personal-project-card carousel-card" data-project="prosalud">
                    <img src="~/images/projects/proSalud/cover.webp"
                         alt="prosalud">
                    <div class="personal-project-content">
                        <h3 data-translate="projects.prosalud.name">ProSalud</h3>
                        <p data-translate="projects.prosalud.shortDescription">Sistema de Salud para hospitales</p>
                    </div>
                </div>
            </div>

            <!-- PAGINACIÓN -->
            <div class="carousel-pagination carousel-personal-project-pagination">
                <span class="current">1</span>/<span class="total">5</span>
            </div>
        </div>
    </div>
</section>



@* Información de las tarjetas *@
